{
  "name": "webpack-es6",
  "version": "1.0.0",
  "description": "starter kit for webpack-es6",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production node --use_strict server.js",
    "go": "npm-run-all --parallel dev-server webpack-watcher",
    "dev-server": "NODE_ENV=development nodemon server.js --ignore node_modules/ --ignore src-client/ --ignore dist",
    "webpack-watcher": "webpack --progress --watch --config webpack.config.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "set-project-name": "node ./src-server/cli/setProjectName.js",
    "build-dist": "webpack",
    "gen-seed-data": "NODE_ENV=development node ./src-server/db/seed.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.15.3",
    "backbone": "1.3.3",
    "bcrypt": "^1.0.2",
    "body-parser": "1.16.1",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "ejs": "2.4.2",
    "express": "4.14.0",
    "express-session": "^1.15.1",
    "googleapis": "^18.0.0",
    "install": "^0.8.7",
    "jquery": "2.2.4",
    "margo-css": "^3.2.1",
    "moment": "^2.18.1",
    "mongoose": "4.8.3",
    "npm-run-all": "4.0.1",
    "passport": "0.3.2",
    "passport-local": "^1.0.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-youtube-audio": "^0.2.10",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "chalk": "1.1.3",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.1",
    "prompt": "1.0.0",
    "sass-loader": "5.0.1",
    "resolve-url-loader": "2.0.0",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.8.4"
  }
}
